<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.paulina.wisehome">

    <application
        android:name=".base.ApplicationContext"
        android:allowBackup="true"
        android:icon="@drawable/patient_avatar"
        android:label="@string/app_name"
        android:roundIcon="@drawable/patient_avatar"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.example.paulina.wisehome.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.paulina.wisehome.rooms.RoomsActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>

    </application>
</manifest>
